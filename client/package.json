  "scripts": {
    "@comment codegen": "Permet de créer tout les types typescript résultant de notre consommation du schéma graphql. La config se trouve dans codegen.ts",
    "codegen": "graphql-codegen",
    "dev": "cross-env PORT=3001 rsbuild dev --open",
    "preview": "rsbuild preview",
    "build": "env-cmd -f .env.production rsbuild build",
    "build-doctor": "cross-env RSDOCTOR=true env-cmd -f .env.production rsbuild build",
    "lint": "cross-env eslint ./src/**/*.{ts,tsx}",
    "lint:report:html": "cross-env eslint ./src/**/*.{ts,tsx} --output-file eslint_report.html --format html ",
    "lint:report:json": "cross-env eslint ./src/**/*.{ts,tsx} --output-file eslint_report.json --format json ",
    "build-ci:production": "cross-env NODE_OPTIONS=--max-old-space-size=4096 CI=true env-cmd -f .env.production rsbuild build",
    "build-ci-pdos1:production": "cross-env NODE_OPTIONS=--max-old-space-size=4096 CI=true env-cmd -f .env.production-pdos1 rsbuild build",
    "build-ci-pdos2:production": "cross-env NODE_OPTIONS=--max-old-space-size=4096 CI=true env-cmd -f .env.production-pdos2 rsbuild build",
    "build:staging": "env-cmd -f .env.staging rsbuild build",
    "build-ci:staging": "CI=true env-cmd -f .env.staging rsbuild build",
    "test": "vitest --coverage --ui",
    "test-ci": "CI=true vitest --run",
    "test-e2e": "cross-env USER_EMAIL=... USER_PASSWORD=... USER_FOLDER=PAD_ID_32 BASE_URL=http://localhost:3001  env-cmd -f .env.development",
    "test-ui": "playwright test --ui",
    "tsc-ci": "cross-env NODE_OPTIONS=--max-old-space-size=8192 tsc -p tsconfig.ci.json",
    "tsc": "cross-env NODE_OPTIONS=--max-old-space-size=8192 tsc",
    "build-storybook": "cross-env NODE_OPTIONS=--max_old_space_size=4096 storybook build",
    "serve-storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "test-storybook-update": "test-storybook -u",
    "serve-and-test-storybook": "cross-env TEST_STORYBOOK=true JEST_JUNIT_OUTPUT_DIR=tests-results JEST_JUNIT_OUTPUT_NAME=storybook-junit.xml NODE_OPTIONS=--max_old_space_size=4096 concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\"  \"storybook build --test && http-server storybook-static --port 6006 --silent\" \"wait-on tcp:127.0.0.1:6006 && test-storybook --junit \"",
    "chromatic": "chromatic --project-token=XXX",
    "eslint-nibble": "eslint-nibble src/utilities/*",
    "//predocker-snapshots": "echo 'Build une image docker à partir de Dockerfile, puis fait tourner un container à partir de cette image. Plus de détails sur DevDocs.'",
    "predocker-snapshots": "docker build --tag tester-image . && docker run --name tester-container -d -i -t tester-image ../../bin/sh ",
    "//docker-snapshots": "echo 'Lance la commande serve-and-test-storybook sur le container instancié par predocker-snapshots'",
    "docker-snapshots": "docker exec tester-container yarn serve-and-test-storybook & exit 0",
    "//postdocker-snapshots": "echo 'Copie les snapshots sur l'hôte, arrête le container et le supprime'",
    "postdocker-snapshots": "docker cp tester-container:/home/app/snapshots/ ./ && docker stop tester-container && docker rm tester-container"
  },
